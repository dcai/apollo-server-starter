{
  "name": "apollo-server-starter",
  "version": "1.0.1",
  "description": "",
  "main": "server/index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/dcai/apollo-server-starter.git"
  },
  "scripts": {
    "start": "NODE_ENV=production node server/index.js",
    "dev": "NODE_ENV=development nodemon server/index.js",
    "pretest": "npm run lint",
    "posttest": "npm run format",
    "lint": "eslint ./server --cache --fix",
    "format": "prettier --write server/*",
    "knex": "knex",
    "migrate:up:dev": "NODE_ENV=development knex migrate:up",
    "migrate:up:prod": "NODE_ENV=production knex migrate:up",
    "migrate:down:dev": "NODE_ENV=development knex migrate:down",
    "migrate:down:prod": "NODE_ENV=production knex migrate:down",
    "seed:dev": "NODE_ENV=development knex seed:run",
    "seed:prod": "NODE_ENV=production knex seed:run",
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test": "npm run test:jest --"
  },
  "author": "Dongsheng Cai <d@tux.im> (http://dongsheng.org/)",
  "license": "BSD-2-Clause",
  "dependencies": {
    "apollo-server-express": "^3.0.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^15.5.1",
    "graphql-tools": "^7.0.5",
    "knex": "^0.95.7",
    "pg": "^8.6.0",
    "ramda": "^0.27.1",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/eslint-parser": "^7.14.7",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "faker": "^5.5.3",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2"
  }
}
